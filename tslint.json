{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "no-console":true,
        "ordered-imports":false,
        "prefer-const":false,
        "max-line-length":false,
        "no-shadowed-variable":false,
        "align":false,
        "noImplicitAny": false,
        "interface-name": [true, "never-prefix"],
        "one-variable-per-declaration":true
    },
    "rulesDirectory": []
}